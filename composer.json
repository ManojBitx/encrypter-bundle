{
  "name": "manojx/encrypter-bundle",
  "description": "A Symfony bundle to encrypt and decrypt data using AES-256-CBC encryption.",
  "keywords": [
    "symfony",
    "bundle",
    "encrypter",
    "encryption",
    "decryption",
    "aes-256-cbc"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "ManojX\\EncrypterBundle\\": "src/",
      "ManojX\\EncrypterBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Manoj Kumar",
      "email": "hello@manojx.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "symfony/framework-bundle": "^6.0 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  }
}
